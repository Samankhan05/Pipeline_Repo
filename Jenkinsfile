pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build_Stage') {
            steps {
                echo 'Building'
            }
        }
        stage('Test_Stage') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy_Stage') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Release_Stage') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
